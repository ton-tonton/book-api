{
  "extends": "airbnb",
  "plugins": [
    "import"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "new-cap": ["error", { "capIsNewExceptions": ["Router"] }],
    "no-console": "off",
    "import/no-extraneous-dependencies": [
      "error",
      {"devDependencies": true}
    ],
    "no-unused-vars": "warn"
  }
}
